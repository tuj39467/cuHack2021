<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="424dp"
        android:layout_height="580dp"
        android:layout_marginBottom="304dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/a" />

    <Spinner
        android:id="@+id/selectEvent"
        android:layout_width="249dp"
        android:layout_height="70dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="314dp"
        android:dropDownSelector="@android:color/background_dark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="331dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="349dp"
        android:text="Welcome to the super bowl LV trivia"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bay"
        android:layout_width="224dp"
        android:layout_height="63dp"
        android:layout_marginStart="113dp"
        android:layout_marginBottom="251dp"
        android:text="Tampa Bay Buccaneers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/chiefs"
        android:layout_width="176dp"
        android:layout_height="62dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="392dp"
        android:text="Kansas City Chiefs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="248dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/stadium2" />

</androidx.constraintlayout.widget.ConstraintLayout>